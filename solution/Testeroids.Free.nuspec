<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Testeroids.Free</id>
    <version>$version$</version>
    <title>Testeroids Unit Testing Framework</title>
    <authors>Testeroids</authors>
    <owners>pombeirp,salfab</owners>
    <licenseUrl>https://github.com/Testeroids/Testeroids/blob/master/LICENSE.md</licenseUrl>
